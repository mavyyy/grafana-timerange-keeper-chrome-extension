{
  "manifest_version": 2,
  "name": "Grafana timerange keeper",
  "description": "Keep specified grafana timerange",
  "version": "1.0",
  "icons": {
      "32": "icons/32.png",
      "48": "icons/48.png",
      "128": "icons/128.png"
  },
  "content_scripts": [{
      "matches": ["http://*/*", "https://*/*" ],
      "js": ["content_scripts.js"]
  }],
  "browser_action": {
      "default_icon": "icons/32.png",
      "default_title": "",
       "default_popup": "popup/popup.html"
  },
  "permissions": [
      "tabs",
      "storage",
      "http://*/*",
      "https://*/*"
  ]
}
